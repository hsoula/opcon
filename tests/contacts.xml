<?xml version="1.0"?>
<test>
    <contact label="fulldescription">
        <!-- Data NOT for the user to see -->
        <unit>test unit</unit>
        <rating>0</rating>
        
        <!-- Can be: unknown, new, lost, current ...-->
        <status>current</status>
        
        <!-- Last update to the contact -->
        <timestamp type="datetime">3/1/2010 0800</timestamp>
        
        <!-- Information fields. Only non-empty one should be provided. -->
        <fields>
            <!-- Identity -->
            <TOE>infantry</TOE>
            <side>RED</side>
            <size>Coy</size>
            <higher_formation>A/1-501</higher_formation>
            <identity>3/A/1-501</identity>
            <augmentation>(+)</augmentation>
            
            <!-- Actual Sighting. -->
            <equipment count="10" category="personel">M16/M4</equipment>
            <equipment count="4" category="vehicle">HMVEE</equipment>
            
            <!-- Capability and intentions -->
            <location type="coordinates">33T UG 2500</location>
            <stance>deployed</stance>
            <activity>redeploying</activity>
            <course>0</course>
            <speed>0</speed>
            <range_min>800</range_min>
            <range_max>1000</range_max>
            <bearing_left>-20</bearing_left>
            <bearing_right>10</bearing_right>
            <altitude>0</altitude>
            
            <!-- Other factors that are more analytical and apply best to friends. --> 
            <casualty_level>100</casualty_level>
            <morale>good</morale>
            <fatigue>good</fatigue>
            <suppression>good</suppression>
            <supply_level>100</supply_level>
            
            <!-- Free Text for umpire, and players-->
            <notes>This section is made of free text.</notes>
            
        </fields>
    </contact>
</test>